{
  "name": "@chainlit/components",
  "private": true,
  "version": "0.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.mjs",
      "require": "./dist/src/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts theme/index.ts hooks/index.ts --dts --format=esm,cjs --clean --dts",
    "build:watch": "tsc && vite build --watch",
    "lint": "eslint . --ext .ts,.tsx && tsc --noemit",
    "format": "prettier **/*.{ts,tsx} --write --loglevel error",
    "test": "vitest run"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/lab": "^5.0.0-alpha.122",
    "@mui/material": "^5.14.7",
    "formik": "^2.4.3",
    "mui-chips-input": "^2.1.3",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^8.0.7",
    "react-resizable": "^3.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1",
    "swr": "^2.2.2",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.2.0",
    "vitest": "^0.34.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/react-resizable": "^3.0.4",
    "@types/react-syntax-highlighter": "^15.5.7",
    "jsdom": "^22.1.0",
    "tsup": "^7.2.0",
    "vite": "^4.4.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
